@model List<SkillsTest.Models.Answer>
@using SkillsTest.Models.Business;

@{
    Layout = "_Layout";
}

@foreach (var question in ViewBag.Questions)
{
    <strong>
        @question.Content
    </strong>
    <br />

    TopBusiness top = new TopBusiness();
    var answers = top.GetListAnswer(@question.Id);

    @foreach (var item in answers)
    {
        <input type="radio" value="@(item.Id)" name="Question@(item.Id)">
        <lable>Hahahahahahahahhaahaha</lable>
    }
}
